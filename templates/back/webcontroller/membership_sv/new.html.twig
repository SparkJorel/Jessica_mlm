{% extends 'admin_dashboard.html.twig' %}
{% block title %} JTWC - Enregistrement des volumes de SV d'un membership {% endblock %}
{% block main %}
    <h3><i class="fa fa-angle-right"></i>Enregistrement des volumes de SV d'un membership</h3>
    <div class="row mb">
        <div class="form-panel">
            <div class="alert alert-success hide"></div>
            {{ form_start(form, {attr: {'id': 'form', 'class': 'form-horizontal style-form'}}) }}
            <div class="form-group">
                {{ form_label(form.membership, null, { 'label_attr':  {
                    'class': 'col-lg-3 control-label'
                }}) }}
                <div class="col-lg-9">
                    {{ form_errors(form.membership) }}
                    {{ form_widget(form.membership, { 'attr':  {
                        'class': 'form-control'
                    }}) }}
                </div>
            </div>
            <div class="form-group">
                {{ form_label(form.svGroupe, null, { 'label_attr':  {
                    'class': 'col-lg-3 control-label'
                }}) }}
                <div class="col-lg-9">
                    {{ form_errors(form.svGroupe) }}
                    {{ form_widget(form.svGroupe, { 'attr':  {
                        'class': 'form-control'
                    }}) }}
                </div>
            </div>
            <div class="form-group">
                {{ form_label(form.svProduct, null, { 'label_attr':  {
                    'class': 'col-lg-3 control-label'
                }}) }}
                <div class="col-lg-9">
                    {{ form_errors(form.svProduct) }}
                    {{ form_widget(form.svProduct, { 'attr':  {
                        'class': 'form-control'
                    }}) }}
                </div>
            </div>
            <div class="col-lg-offset-5 col-lg-5">
                <input type="submit" name="submit" class="btn btn-theme" value="Submit" />
            </div>
            {{ form_end(form) }}
        </div>
    </div>
{% endblock %}
{% block javascripts %}
{{ parent() }}
    {{ encore_entry_script_tags('validate') }}
{% endblock %}
